      <Label layoutX="38.0" layoutY="102.0" text="Пароль" />
      <Button fx:id="btnEnter" layoutX="67.0" layoutY="174.0" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="24.0" prefWidth="88.0" text="Сохранить" />
