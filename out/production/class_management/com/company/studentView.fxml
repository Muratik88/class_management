      <TextField fx:id="txtName" layoutX="60.0" layoutY="43.0" prefHeight="24.0" prefWidth="173.0" promptText="Ф.И.О. Студента" />
      <TextField fx:id="txtPhone" layoutX="60.0" layoutY="71.0" prefHeight="24.0" prefWidth="174.0" promptText="Телефон" />
